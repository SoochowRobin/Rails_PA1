<p id="notice"><%= notice %></p>

<h1>Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Subject name</th>
      <th>Segment</th>
      <th>Segment name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.subject_id %></td>
        <td><%= subject.subject_name %></td>
        <td><%= subject.segment_id %></td>
        <td><%= subject.segment_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subject', new_subject_path %>
